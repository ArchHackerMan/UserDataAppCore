@page
@model IndexModel
@{
    ViewData["Title"] = "User Data";
}

<h2>User Data Form</h2>

<form method="post">
    <div>
        <label>Name:</label>
        <input asp-for="UserName" />
    </div>
    <div>
        <label>Email:</label>
        <input asp-for="UserEmail" />
    </div>
    <button type="submit">Submit</button>
</form>


